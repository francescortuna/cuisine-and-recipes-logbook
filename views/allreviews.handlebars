<div class="cards">
  {{#each reviews as |review|}}
  <div class="card">
    <div class="card-body">
      <h4 class="card-title text-center">Rating:
        {{review.reviewer_score}}/10</h4>
      <p class="card-text"><strong>Reviewer name: </strong>{{review.reviewer_name}}</p>
      <p class="card-text"><strong>Description:</strong>
        {{review.review_description}}</p>
      <p class="card-text">Reviewed {{format_time review.createdAt}}</p>
    </div>
  </div>
  {{/each}}
</div>